{
  "name": "mr-crud",
  "version": "0.1.0",
  "private": true,
  "description": "Simple App that uses the GitHub API to CRUD files. Based on Node.js, Express, Vue.js and Vuetify",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode dev --watch",
    "start": "node src/node/server.js",
    "watch": "nodemon src/node/server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "connect-redis": "^4.0.3",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "global": "^4.4.0",
    "ip": "^1.1.5",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "redis": "^2.8.0",
    "vue": "^2.6.10",
    "vue-native-websocket": "^2.0.13",
    "vue-router": "^3.1.3",
    "vuetify": "^1.5.19",
    "vuex": "^3.1.1",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "~4.5.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.8",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "14.x"
  },
  "homepage": "https://mr-crud.herokuapp.com/"
}
